<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Header4 -->
    <style name="LLPHeader4">
        <item name="android:textSize">28sp</item>
        <item name="android:fontFamily">@font/notosans_extrabold</item>
        <item name="android:textColor">@color/llp_black</item>
    </style>

    <!-- Header5 -->
    <style name="LLPHeader5">
        <item name="android:textSize">24sp</item>
        <item name="android:fontFamily">@font/notosans_extrabold</item>
        <item name="android:textColor">@color/llp_black</item>
    </style>

    <!-- ButtonText1 -->
    <style name="LLPButtonText1">
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/notosans_extrabold</item>
        <item name="android:textColor">@color/llp_black</item>
    </style>

    <style name="LLPCustomProgressBar" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/llp_custom_progress_bar</item>
    </style>

    <style name="LLPCustomDisableProgressBar" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/llp_custom_disable_progress_bar</item>
    </style>

    <style name="LLPTextInterMedium">
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textColor">@color/llp_body_text_1</item>
    </style>

    <style name="LLPTextInterBold">
        <item name="android:fontFamily">@font/inter_semibold</item>
        <item name="android:textColor">@color/llp_black_2</item>
    </style>

</resources>